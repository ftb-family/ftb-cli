{
  "name": "react-library",
  "version": "0.0.0",
  "description": "A tool for publish npm library to npm",
  "main": "dist/ftb-library.min.js",
  "scripts": {
    {{#if jest}}
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    {{/if}}
    {{#if eslint}}
    "fix": "eslint --fix 'src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.js'",
    {{/if}}
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --config build/webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.0",
    {{#if eslint}}
    "babel-eslint": "^10.0.2",
    {{/if}}
    {{#if jest}}
    "babel-jest": "^24.8.0",
    {{/if}}
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.1",
    "commander": "^2.19.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    {{#if jest}}
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    {{/if}}
    {{#if eslint}}
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    {{/if}}
    "html-webpack-plugin": "^3.2.0",
    {{#if less}}
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    {{/if}}
    "inquirer": "^6.2.2",
    {{#if jest}}
    "jest": "^24.5.0",
    {{/if}}
    "mini-css-extract-plugin": "^0.5.0",
    {{#if scss}}
    "node-sass": "^4.11.0",
    {{/if}}
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    {{#if eslint}}
    "prettier": "^1.16.4",
    {{/if}}
    {{#if scss}}
    "sass-loader": "^7.1.0",
    {{/if}}
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack-auto-find-port": "0.0.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "browserslist": [
    "> 1%",
    "Firefox ESR",
    "last 2 versions",
    "ie >= 9"
  ]
}
