    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.1",
    "commander": "^2.19.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
